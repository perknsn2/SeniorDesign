package org.ishan.vms.model;

import java.util.Date;
import javax.xml.bind.annotation.*;

@XmlRootElement
public class Data {
	
	private int id;
	private float data;
	private Date date;
	
	public Data(){
		
	}
	
	public Data(int id, float data){
		this.id = id;
		this.data = data;
		this.date = new Date();
	}
	
	
	public float getData() {
		return data;
	}
	public void setData(int data) {
		this.data = data;
	}
	public Date getDate() {
		return date;
	}
	public void setDate(Date date) {
		this.date = date;
	}

	
	
}
